#!/usr/bin/expect
set timeout 10
spawn /bin/sh -c "$argv"
expect {
    "Verify return code: 0 " {
        send "client hello\r"
        expect "client hello" {
            expect "client hello" {
                expect -re ".+" {
                    close
                    exit 0
                }
            }
        }
    }
}
exit 1
